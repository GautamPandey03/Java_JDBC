pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Buiding project'
            }
        }
stage("build & SonarQube analysis") {
            agent any
            steps {
              withSonarQubeEnv('sq1') {
                sh 'mvn sonar:sonar'
              }
            }
        }
     }
}
