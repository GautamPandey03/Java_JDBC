pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Buiding project'
            }
        }
        stage('SCM') {
        steps
        {
            git 'https://github.com/GautamPandey03/Java_JDBC'
        }
       }
  stage('SonarQube analysis') {
  steps
  {
    withSonarQubeEnv(credentialsId: '7438283d-2d4b-4ddb-a78f-a8dd696e053f', installationName: 'Sonarqube') { // You can override the credential to be used
      sh 'mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.7.0.1746:sonar'
    }
    echo 'Compiling..'
   }
  }
 }
}
